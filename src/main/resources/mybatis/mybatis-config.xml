<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE configuration 
    PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-config.dtd"> 
<configuration>
	<settings>
		<setting name="callSettersOnNulls" value="true" />
		<setting name="jdbcTypeForNull" value="NULL" /> 
	</settings>

	<typeAliases>
		<typeAlias alias="MemberDTO" type="com.hoiae.mygoods.member.dto.MemberDTO" />
		<typeAlias alias="PaymentDTO" type="com.hoiae.mygoods.payment.dto.PaymentDTO"/>
		<typeAlias alias="OrderDTO" type="com.hoiae.mygoods.payment.dto.OrderDTO"/>
		<typeAlias alias="ProductDTO" type="com.hoiae.mygoods.product.dto.ProductDTO"/>
		<typeAlias alias="CharacterDTO" type="com.hoiae.mygoods.product.dto.CharacterDTO"/>
	</typeAliases>

	<mappers>
		<mapper resource="MemberMapper.xml" />

		<mapper resource="ProductMapper.xml" />

	</mappers>

</configuration>